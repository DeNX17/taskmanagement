{
   "name": "nest",
   "version": "0.0.1",
   "description": "",
   "author": "",
   "license": "MIT",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "node dist/main",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "prestart:prod": "npm run build",
      "postinstall": "npm run prestart:prod",
      "lint": "tslint -p tsconfig.json -c tslint.json",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.json",
      "migrations:create": "yarn typeorm:cli migration:create -n",
      "migrations:list": "yarn typeorm:cli migration:show",
      "migrations:up": "yarn typeorm:cli migration:run",
      "migrations:down": "yarn typeorm:cli migration:revert"
   },
   "dependencies": {
      "@nestjs/common": "^6.7.2",
      "@nestjs/core": "^6.7.2",
      "@nestjs/jwt": "^6.1.1",
      "@nestjs/passport": "^6.1.0",
      "@nestjs/platform-express": "^6.7.2",
      "@nestjs/typeorm": "^6.2.0",
      "@types/bcryptjs": "^2.4.2",
      "@types/node-fetch": "^2.5.5",
      "@types/uuidv4": "^5.0.0",
      "bcryptjs": "^2.4.3",
      "class-transformer": "^0.2.3",
      "class-validator": "^0.11.0",
      "mysql": "^2.14.1",
      "node-fetch": "^2.6.0",
      "passport": "^0.4.0",
      "passport-jwt": "^4.0.0",
      "pg": "^7.18.2",
      "reflect-metadata": "^0.1.10",
      "rimraf": "^3.0.0",
      "rxjs": "^6.5.3",
      "typeorm": "0.2.20",
      "uuidv4": "^6.0.6"
   },
   "devDependencies": {
      "@nestjs/cli": "^6.9.0",
      "@nestjs/schematics": "^6.7.0",
      "@nestjs/testing": "^6.7.1",
      "@types/express": "^4.17.1",
      "@types/jest": "^24.0.18",
      "@types/node": "^8.0.29",
      "@types/supertest": "^2.0.8",
      "jest": "^24.9.0",
      "prettier": "^1.18.2",
      "supertest": "^4.0.2",
      "ts-jest": "^24.1.0",
      "ts-loader": "^6.1.1",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.9.0",
      "tslint": "^5.20.0",
      "typescript": "3.3.3333"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".spec.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "coverageDirectory": "./coverage",
      "testEnvironment": "node"
   }
}
